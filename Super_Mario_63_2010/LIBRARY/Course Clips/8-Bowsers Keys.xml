				if (keycount == 2) {
					_root.heldItemMessage = "You have 3 Bowser Keys!  One more!";
				}
				if (keycount ==3) {
					_root.heldItemMessage = "You have all 4 Bowser Keys!";
				}
			}
			_root[key] = true;
			//
			gotoAndStop(6);
		}
	} else {
		count++;
	}
